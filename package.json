{
	"name": "dynamic-s-tokens-simple-tiers",
	"version": "1.0.0",
	"private": true,
	"description": "Template repository for using TypeScript and Solidity",
	"main": "index.js",
	"scripts": {
		"test": "hardhat test",
		"pretest": "yarn generate",
		"generate": "hardhat compile",
		"pregenerate": "yarn clean",
		"lint": "yarn lint:eslint && yarn lint:solhint && yarn lint:format",
		"lint:eslint": "eslint . --ext .ts --fix",
		"lint:solhint": "solhint --fix --max-warnings 0 \"contracts/**/*.sol\"",
		"lint:format": "prettier --write '**/*.{sol,ts,json,md,yml}'",
		"build": "tsc -p tsconfig.build.json",
		"prebuild": "yarn generate",
		"clean": "del 'artifacts' 'typechain-types'",
		"prepare": "husky install"
	},
	"author": "aggre",
	"license": "MPL-2.0",
	"devDependencies": {
		"@nomiclabs/hardhat-ethers": "2.1.1",
		"@nomiclabs/hardhat-etherscan": "3.0.4",
		"@nomiclabs/hardhat-waffle": "2.0.3",
		"@openzeppelin/contracts": "4.8.0",
		"@types/chai": "4.3.4",
		"@types/dotenv": "8.2.0",
		"@types/mocha": "9.1.1",
		"@types/node": "18.11.9",
		"@typescript-eslint/eslint-plugin": "5.44.0",
		"@typescript-eslint/parser": "5.44.0",
		"chai": "4.3.7",
		"del-cli": "4.0.1",
		"dotenv": "16.0.3",
		"eslint": "8.28.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-config-xo": "0.43.1",
		"eslint-config-xo-typescript": "0.44.0",
		"ethereum-waffle": "3.4.4",
		"ethers": "5.7.2",
		"hardhat": "2.9.9",
		"husky": "7.0.4",
		"mocha": "9.2.2",
		"prettier": "2.8.0",
		"prettier-plugin-solidity": "1.0.0",
		"ts-node": "10.9.1",
		"typescript": "4.9.3"
	},
	"dependencies": {
		"@devprotocol/i-s-tokens": "2.0.1",
		"@openzeppelin/contracts-upgradeable": "4.8.0",
		"@typechain/ethers-v5": "10.1.1",
		"@typechain/hardhat": "6.1.4",
		"solhint": "3.3.7",
		"typechain": "8.1.1"
	},
	"directories": {
		"test": "test"
	},
	"repository": {
		"type": "git",
